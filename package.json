{
  "name": "sanity-plugin-internationalized-array",
  "version": "2.0.0",
  "description": "Store localized fields in an array to save on attributes",
  "keywords": [
    "sanity",
    "sanity-plugin"
  ],
  "homepage": "https://github.com/sanity-io/sanity-plugin-internationalized-array#readme",
  "bugs": {
    "url": "https://github.com/sanity-io/sanity-plugin-internationalized-array/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:sanity-io/sanity-plugin-internationalized-array.git"
  },
  "license": "MIT",
  "author": "Sanity.io <hello@sanity.io>",
  "sideEffects": false,
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./lib/index.mjs",
      "require": "./lib/index.js",
      "default": "./lib/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./lib/index.js",
  "module": "./lib/index.esm.js",
  "source": "./src/index.ts",
  "types": "./lib/index.d.ts",
  "files": [
    "lib",
    "sanity.json",
    "src",
    "v2-incompatible.js"
  ],
  "scripts": {
    "prebuild": "npm run clean",
    "build": "pkg build --strict && pkg check --strict",
    "clean": "rimraf lib",
    "format": "prettier --write --cache --ignore-unknown .",
    "link-watch": "plugin-kit link-watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky install",
    "prepublishOnly": "run-s build",
    "watch": "pkg-utils watch --strict"
  },
  "dependencies": {
    "@sanity/icons": "^2.11.7",
    "@sanity/incompatible-plugin": "^1.0.4",
    "@sanity/language-filter": "^4.0.2",
    "@sanity/ui": "^2.1.0",
    "fast-deep-equal": "^3.1.3",
    "lodash": "^4.17.21",
    "suspend-react": "0.1.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@sanity/pkg-utils": "^6.1.0",
    "@sanity/plugin-kit": "^3.1.10",
    "@sanity/semantic-release-preset": "^4.1.7",
    "@types/lodash": "^4.17.0",
    "@types/react": "^18.2.75",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-sanity": "^7.1.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "npm-run-all2": "^5.0.0",
    "prettier": "^3.2.5",
    "prettier-plugin-packagejson": "^2.4.14",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "rimraf": "^4.4.1",
    "sanity": "^3.36.4",
    "semantic-release": "^23.0.7",
    "typescript": "^5.4.4"
  },
  "peerDependencies": {
    "@sanity/ui": "^2.1.0",
    "react": "^18",
    "react-dom": "^18",
    "sanity": "^3.36.4",
    "styled-components": "^6.1"
  },
  "engines": {
    "node": ">=14"
  },
  "sanityPlugin": {
    "verifyPackage": {
      "eslintImports": false,
      "dependencies": false
    }
  }
}
